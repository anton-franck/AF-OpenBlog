{
  "kind": "collectionType",
  "collectionName": "blogentries",
  "info": {
    "singularName": "blogentry",
    "pluralName": "blogentries",
    "displayName": "Blogentry"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "labels": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::label.label",
      "mappedBy": "blogentry"
    },
    "slug": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "blogimage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "components": {
      "type": "dynamiczone",
      "components": [
        "components.herobanner",
        "components.richtext",
        "components.imagebanner",
        "components.border"
      ]
    },
    "seotitle": {
      "type": "string",
      "required": true
    },
    "seodescription": {
      "type": "text"
    }
  }
}
