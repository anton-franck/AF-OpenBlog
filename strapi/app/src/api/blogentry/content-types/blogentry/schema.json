{
  "kind": "collectionType",
  "collectionName": "blogentries",
  "info": {
    "singularName": "blogentry",
    "pluralName": "blogentries",
    "displayName": "Blogentry"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "label": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::label.label",
      "inversedBy": "blogentries"
    },
    "slug": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "blogimage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "components": {
      "type": "dynamiczone",
      "components": [
        "components.herobanner",
        "components.richtext",
        "components.imagebanner",
        "components.border",
        "components.button",
        "components.accordion",
        "components.cardsbanner"
      ]
    },
    "seotitle": {
      "type": "string",
      "required": true
    },
    "seodescription": {
      "type": "text"
    },
    "showentrybanner": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "blogentries": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::blogentry.blogentry",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "showentrybanner"
            },
            true
          ]
        }
      }
    }
  }
}
