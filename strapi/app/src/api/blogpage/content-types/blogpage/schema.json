    },
    "favblogs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::blogentry.blogentry"
    },
    "seotitle": {
      "type": "string",
      "default": "OpenBlog",
      "required": true
    },
    "seodescription": {
      "type": "text"
    }
  }
}
